
<%- include ('../navbarpage/navbar') %>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>


<table class="table table-hover table-responsive table-bordered">
  <thead class="thead-dark">
    <tr>
      <h3>User</h3>
     </tr>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Email Address</th>
      <th scope="col">Organization</th>
    </tr>
  </thead>
  
  <% 
  if(userData.length!=0){
    var i=1;
    userData.forEach(function(users){
    %>
  <tr>

    <td><%=users.username %></td>
    <td><%=users.email %></td>
    <td><%=users.organization.name %></td>
  </tr>
  <%  i++; }) %>
  <% } else{ %>
  <tr>
    <td colspan="7">No Data Found</td>
  </tr>
  <% } %>
</table>
<table class="table table-hover table-responsive table-bordered">
    <thead>
        <tr>
         <h3>Devices</h3>
        </tr>
      <tr>
        <th scope="col">Device Name</th>
        <th scope="col">Device uuid</th>
        <th scope="col">Device Details</th>
      </tr>
    </thead>
    
    <% 
    if(devices.length!=0){
      var i=1;
      devices.forEach(function(devices){
      %>
      <tbody class="tbody">
    <tr>
  
      <td><%=devices.name %></td>
      <td><%=devices.deviceUuid %>
 
   
      <td><a href="<%= link %><%=devices.deviceUuid %>"  class="text-center border border-light p-5"  role="button">Details</a></td>
     
    </tr>
    <%  i++; }) %>
    <% } else{ %>
    <tr>
      <td colspan="7">No Data Found</td>
    </tr>
    <% } %>
  </table>
  


    <%if(typeof message == "undefined"){%>
        <div></div>
    <% } else { %>
        <div class="error"> <%= message%> </div> 
    <%}%>

</div>
</div>
<button id="btn-back" class="btn btn-outline-primary" data-toggle="button" onclick="goBack()">Go Back</button>

<script>
  function goBack() {
    window.history.back();
  }
  </script>
<%- include ('../footerpage/footer') %>



