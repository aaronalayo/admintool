<%- include ('../navbarpage/navbar') %>




<table class="table table-hover table-responsive table-bordered">
    <thead>
        <tr>
         <h3></p>Sensor <%=sensor %></h3>
        </tr>
      <tr>
        <th scope="col">Sensor Uuid</th>
        <th scope="col">Sensor Id</th>
        <th scope="col">Name</th>
        <th scope="col">Sensor Type</th>
        <th scope="col">Description</th>
        <th scope="col">Unit</th>
        <th scope="col">Calibration Uuid</th>


      </tr>
    </thead>
    
    <% 
    if(sensorDetail.length!=0){
      var i=1;
      sensorDetail.forEach(function(sensor){
      %>
    <tr>
      <td><%=sensor.sensorUuid %></td>
      <td><%=sensor.sensorId %></td>
      <td><%=sensor.name %></td>
      <td><%=sensor.sensorType %></td>
      <td><%=sensor.description %></td>
      <td><%=sensor.unit %></td>
      <td><%=sensor.calibrationUuid %></td>
     
      
     
    </tr>
    <%  i++; }) %>
    <% } else{ %>
    <tr>
      <td colspan="7">No Data Found</td>
    </tr>
    <% } %>
  </table>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.slim.js" integrity="sha256-Dul4c09cdrWKXVtallPxF558lwxMwCC8dXJdZ0PVW54=" crossorigin="anonymous"></script>
  
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.3/Chart.bundle.min.js"></script>
  
  
  <div class="chart-container" style="position: relative; height:40vh; width:65vw">
    <canvas id="chart"></canvas>
  </div>
  
  <script>   
           
  
  var ctx = document.getElementById('chart').getContext('2d');
  
      var chart = new Chart(ctx, {
      type: 'line',
      data: {
          labels:<%-JSON.stringify(labels)%>,
          datasets: [{
              label:<%-JSON.stringify(sensor)%>,
              data: <%-JSON.stringify(dataset)%>,
              backgroundColor: [
                  // 'rgba(255, 99, 132, 0.2)',
                  // 'rgba(54, 162, 235, 0.2)',
                  // 'rgba(255, 206, 86, 0.2)',
                  'rgba(75, 192, 192, 0.2)',
                  // 'rgba(153, 102, 255, 0.2)',
                  'rgba(255, 159, 64, 0.2)'
              ],
              borderColor: [
                  'rgba(255, 99, 132, 1)',
                  // // 'rgba(54, 162, 235, 1)',
                  // // 'rgba(255, 206, 86, 1)',
                  // 'rgba(75, 192, 192, 1)',
                  // 'rgba(153, 102, 255, 1)',
                  // 'rgba(255, 159, 64, 1)'
              ],
              borderWidth: 1
          }]
      },
      options: {
          responsive: true, 
          maintainAspectRatio: false,
          tooltips: {
             
          },
          hover: {
              mode: 'dataset',
              mode: 'point',
          },
          legend: {
              labels: {
              fontSize: 18
              }
          },
          title: {
              display: true,
              text: 'Measurements from sensor '
      },
          scales: {
              xAxes: [{
                  
                  scaleLabel: {
                      display: true,
                  },
                  ticks: {
                  callback: function(value) { 
                  return new Date(value).toLocaleDateString('dk-DK', {hour:'numeric', minute:'numeric'}); 
      },
  },
          
       }],
              
              yAxes: [{
                  
                  ticks: {
                      beginAtZero: true
             
                  }
              }],
              
          }
      }
      
  });
  
  </script>

     
     
         <div class="form-group col-md-2">
      <%if(typeof message == "undefined"){%>
          <div></div>
        <% } else { %>
          <div class="error" role="alert"> <%= message%> </div> 
        <%}%>
      </div>

   

  <button class="btn btn-outline-primary" data-toggle="button" onclick="goBack()">Go Back</button>
<script>
function goBack() {
  window.history.back();
}
</script>

















