  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.1.3/Chart.bundle.min.js"></script>

  <div class="card-body"><canvas id="myPieChart" width="150%" height="50"></canvas></div>
  <script>   
    Chart.defaults.global.defaultFontFamily = '-apple-system,system-ui,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif';
    Chart.defaults.global.defaultFontColor = '#292b2c';
    var ctx = document.getElementById("myPieChart");
    var myPieChart = new Chart(ctx, {
    type: 'pie',
    data: {
      labels: ["Users", "User Role", "Admin Role", "Organizations"],
      datasets: [{
        data: [<%-JSON.stringify(userNumber)%>,<%-JSON.stringify(userRole)%>, <%-JSON.stringify(adminRole)%>, <%-JSON.stringify(organizations)%>],
        backgroundColor: ['#007bff', '#dc3545', '#ffc107', '#28a745'],
      }],
    },
  });
  </script>

